FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//Only if you use swak4foam
/*
libs
(
	"libgroovyBC.so"
	"libOpenFOAM.so"		//use if you have problems with parafoam
