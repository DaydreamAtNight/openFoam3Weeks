FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       surfaceScalarField;
    location    "4";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

oriented        oriented;

internalField   uniform 8;

boundaryField
{
    sides
    {
        type            calculated;
        value           nonuniform List<scalar> 2(8 -8);
    }
    empty
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
}


