FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (1 0 0)  
    volScalarFieldValue T 0
);

regions
(
    boxToCell
    {
        box (-0.5 -1 -1) (0.5 1 1);
        fieldValues
        (
            volScalarFieldValue T 1
        );
    }
);


